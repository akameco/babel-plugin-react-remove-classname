{
  "name": "babel-plugin-react-remove-classname",
  "version": "0.1.1",
  "description": "babel plugin for remove classname",
  "license": "MIT",
  "repository": "akameco/babel-plugin-react-remove-classname",
  "author": "akameco <akameco.t@gmail.com> (akameco.github.io)",
  "engines": {
    "node": ">=4"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "add-contributor": "all-contributors add",
    "build": "babel src -d lib --ignore *.test.js,__fixtures__",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "flow": "flow",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --ci --runInBand",
    "test:ci": "npm run validate",
    "validate": "npm run lint && npm run flow && npm run test"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "ast"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "all-contributors-cli": "^6.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-log": "^2.0.0",
    "babel-plugin-tester": "^5.5.2",
    "eslint": "^5.13.0",
    "eslint-config-precure": "^4.3.3",
    "flow-bin": "^0.92.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "string-snapshot-serializer": "^1.0.1"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
